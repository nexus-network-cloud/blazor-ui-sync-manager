<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
	  <TargetFramework>net6.0</TargetFramework>
	  <Version>1.0.0</Version>
	  <Company>Nexus-Network-Cloud</Company>
	  <RepositoryUrl>https://github.com/nexus-network-cloud/blazor-ui-sync-manager</RepositoryUrl>
	  <RepositoryType>git</RepositoryType>
	  <PackageId>BlazorUISyncManager</PackageId>
	  <Authors>Morkeon</Authors>
	  <ImplicitUsings>enable</ImplicitUsings>
	  <Nullable>enable</Nullable>
	  <RootNamespace>NexusNetworkCloud.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
  </PropertyGroup>


  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="6.0.6" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\" />
  </ItemGroup>

</Project>
